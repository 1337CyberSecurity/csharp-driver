{
  "version": "2.0.0-beta1",
  "name": "Dse",
  "authors": [ "DataStax" ],
  "title": "DataStax Enterprise C# Driver",
  "description": "Built on top of DataStax C# driver for Apache Cassandra, it provides additional features designed for DataStax Enterprise.",
  "copyright": "Copyright © 2017 by DataStax",
  "packOptions": {
    "summary": "Built on top of DataStax C# driver for Apache Cassandra, it provides additional features designed for DataStax Enterprise.",
    "licenseUrl": "https://github.com/datastax/csharp-driver/blob/master/LICENSE.txt",
    "tags": [ "cassandra", "apache", "datastax", "driver", "client", "database", "nosql", "dotnet", "netcore", "db", "dse" ],
    "repository": { "url": "https://github.com/datastax/csharp-driver-dse" }
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../build/datastax.snk",
    "xmlDoc": true,
    "nowarn": [ "1591" ],
    "languageVersion": "csharp5"
  },
  "dependencies": {
    "Microsoft.Extensions.Logging": "1.0.0",
    "Microsoft.Extensions.Logging.Abstractions": "1.0.0",
    "Newtonsoft.Json": "9.0.1"
  },
  "frameworks": {
